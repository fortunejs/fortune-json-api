{
  "name": "fortune-json-api",
  "description": "JSON API serializer for Fortune.",
  "version": "2.1.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:fortunejs/fortune-json-api.git"
  },
  "bugs": "https://github.com/fortunejs/fortune-json-api/issues",
  "scripts": {
    "postpublish": "npm run tag",
