    "test": "npm run lint && node test",
    "lint": "eslint lib test"
  },
  "dependencies": {
    "deep-equal": "^1.0.1",
    "inflection": "^1.12.0",
    "uri-templates": "^0.2.0"
  },
  "devDependencies": {
    "ajv": "^4.11.8",
    "chalk": "^2.3.2",
    "deep-equal": "^1.0.1",
    "eslint": "^4.18.2",
    "eslint-config-boss": "^1.0.6",
    "fortune": "^5.4.2",
    "fortune-http": "^1.2.8",
    "tapdance": "^5.0.4"
  },
  "files": [
    "lib/",
    "LICENSE"
  ],
  "main": "lib/index.js",
  "eslintConfig": {
    "extends": "boss",
    "rules": {
      "strict": 0,
      "indent": 0
    }
  },
  "engines": {
    "node": ">=4.2.0"
  },
  "keywords": [
    "json",
    "api",
    "fortune",
    "http",
    "hypermedia",
    "rest",
    "serializer"
